{
    "name": "bujanov/dto-bundle",
    "description": "Symfony simple data transfer object bundle",
    "version": "0.8.0",
    "type": "symfony-bundle",
    "keywords": ["dto", "rest", "api", "bundle", "symfony"],
    "license": "MIT",
    "authors": [{"name": "Bujanov Sergey", "email": "sabujanov@gmail.com"}],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bujanov\\DtoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bujanov\\DtoBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.0-dev"
        }
    }
}
